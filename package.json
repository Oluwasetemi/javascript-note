{
  "name": "javascript-note",
  "type": "module",
  "private": true,
  "scripts": {
    "postinstall": "echo 'Skipping playwright install on CI'",
    "build": "cross-env NODE_OPTIONS='--openssl-legacy-provider --max_old_space_size=8192' slidev build --timeout 60000",
    "build:netlify": "cross-env NODE_ENV=production NODE_OPTIONS='--openssl-legacy-provider --max_old_space_size=4096' slidev build --timeout 60000 --skip-export",
    "dev": "nodemon -e 'md,js,mjs,cjs,json' -w '../../packages/slidev/dist/*.mjs' --exec \"slidev ./slides.md --open=false --log=info --inspect\"",
    "export": "slidev export",
    "export:pdf": "bun run export -- --timeout 120000 --with-toc --per-slide --output ./_export/javascript-note.pdf",
    "export:pptx": "bun run export -- --timeout 120000 --with-clicks false --format pptx --output ./_export/javascript-note.pptx",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@slidev/cli": "52.11.1",
    "@slidev/client": "^52.11.1",
    "@slidev/theme-default": "latest",
    "@vueuse/core": "^14.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@babel/standalone": "^7.28.5",
    "@iconify-json/logos": "^1.2.10",
    "@iconify-json/mdi": "^1.2.3",
    "@slidev/types": "51.1.2",
    "@vue/compiler-sfc": "^3.5.26",
    "cross-env": "^10.1.0",
    "markdown-it-magic-link": "^0.1.4",
    "nodemon": "^3.1.11",
    "playwright-chromium": "^1.57.0",
    "prettier": "^3.7.4",
    "prettier-plugin-slidev": "^1.0.5"
  },
  "license": "MIT",
  "packageManager": "bun@1.3.5"
}
